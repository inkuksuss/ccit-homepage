extends layouts/main
include mixins/videoBlock

block content
    .videoComplain__box
        each item in videoComplain
            +videoBlock({
                id:item.id,
                title:item.title,
                views:item.views,
                videoFile:item.videoUrl
            })
    .photoComplain__box
        input#jsPhotoComplain(type="hidden", value=photoComplain)
    