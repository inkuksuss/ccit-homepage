extends layouts/main
include mixins/photoBlock

block content
    .wrapper
        .up_btn_body
            a.up_btn(href=`/boards${routes.photoUpload}`) 업로드
            .photos
                each item in photolist
                    +photoBlock({
                        id:item.id,
                        title:item.title,
                        views:item.views,
                        photoFile:item.photoUrl
                    })