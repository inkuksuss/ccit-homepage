extends layouts/main
include mixins/videoBlock

block content
    .wrapper
        .up_btn_body
            a.up_btn(href=`/boards${routes.videoUpload}`) 업로드    
            .videos
                each item in videolist
                    +videoBlock({
                        id:item.id,
                        title:item.title,
                        views:item.views,
                        videoFile:item.videoUrl
                    })